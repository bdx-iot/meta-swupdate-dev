if env exists console
then 
	setenv bootargs 'console=${console},${baudrate} root=@@SWUPDATE_STORAGE_DEVICE@@p'${rootpart}' oops=panic panic=5 rootwait rw'
else 
	setenv bootargs 'root=@@SWUPDATE_STORAGE_DEVICE@@p'${rootpart}' oops=panic panic=5 rootwait rw'
fi

run swu_setup
load mmc ${devnum}:${rootpart} @@UBOOT_FITIMAGE_ADDR@@ /boot/fitImage
bootm @@UBOOT_FITIMAGE_ADDR@@
