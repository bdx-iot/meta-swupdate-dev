part u-boot --source rawcopy --sourceparams="file=imx-boot" --ondisk mmcblk --no-table --align ${IMX_BOOT_SEEK}
part /boot --source bootimg-partition --ondisk mmcblk --fstype=vfat --label boot --active --align 8192 --size 64
part / --source rootfs --ondisk mmcblk --fstype=ext4 --label rootfs_A  --align 8192 --extra-space 600 --exclude-path=data/
part / --source rootfs --ondisk mmcblk --fstype=ext4 --label rootfs_B  --align 8192 --extra-space 600 --exclude-path=data/
part /data --source rootfs --rootfs-dir=${IMAGE_ROOTFS}/data --ondisk mmcblk --fstype=ext4 --label data --size 128M --align 8192 --extra-space 0

bootloader --ptable msdos
