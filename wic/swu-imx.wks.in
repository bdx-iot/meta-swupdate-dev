part /boot --source bootimg-partition --ondisk mmcblk --fstype=vfat --label boot --active --align 4096 --size 20
part / --source rootfs --ondisk mmcblk --fstype=ext4 --label rootfs_A  --align 8192 --extra-space 200 --exclude-path=data/ 
part / --source rootfs --ondisk mmcblk --fstype=ext4 --label rootfs_B  --align 8192 --extra-space 200 --exclude-path=data/ 
part /data --source rootfs --rootfs-dir=${IMAGE_ROOTFS}/data --ondisk mmcblk --fstype=ext4 --label data --size 128M --align 8192 --extra-space 0

bootloader --ptable msdos
